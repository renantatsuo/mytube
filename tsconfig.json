{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "paths": {
      "#assets/*": ["./assets/*"],
      "#hooks/*": ["./hooks/*"],
      "#lib/*": ["./lib/*"],
      "#context/*": ["./context/*"],
      "#screens/*": ["./screens/*"],
      "#components/*": ["./components/*"]
    }
  }
}
